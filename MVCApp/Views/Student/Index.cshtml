
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    @{ 
        var employee = ViewData["Student"]
            as MVCApp.Models.StudentModel;
    }  
    <div> 
        <h1>@ViewData["Header"]</h1>
        <table style="font-family:Arial">
            <tr>
                <td>Employee Id</td>
                <td>@employee.EmployeeId</td>
            </tr>
            <tr>
                <td>Employee Name</td>
                <td>@employee.Name</td>
            </tr>
            <tr>
                <td>Employee Address</td>
                <td>@employee.Address</td>
            </tr>
        </table>
    </div>
</body>
</html>
